
@{
    ViewBag.Title = "ListPersNatural";
    List<WebApplication1.Models.PersonaNatural> ListPersNatural = (List<WebApplication1.Models.PersonaNatural>)ViewData["ListPersNatural"];
}
@if (TempData["Mensaje"] != null)
{
    <div class="alert alert-warning fade in">
        <button type="button" class="close close-sm" data-dismiss="alert"> <i class="icon-remove"></i> </button>
        <strong>Advertencia!</strong> @TempData["Mensaje"].ToString()
    </div>
}
<div class="wrapper">
    <div class="page-content">
        <div class="content container">
            <div class="row">
                <div class="col-lg-12">
                    <h2 class="page-title">PERSONAS ENCUESTADAS <small> Listar</small></h2>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="widget">
                        <div class="widget-header">
                            <i class="icon-table"></i>
                            <h3>Lista de personas Naturales</h3>
                        </div>
                        <div class="widget-content">

                            <div class="example_alt_pagination">
                                <div id="container">
                                    <div class="full_width big"></div>
                                    <div id="demo">

                                        @if (ListPersNatural.Count() != 0)
                                        {

                                          
                                                    
                                             
                                            <table cellpadding="0" cellspacing="0" border="0" class="table" id="example">
                                                <thead>
                                                    <tr>
                                                        <th>ID</th>
                                                        <th>DNI</th>
                                                        <th class="hidden-xs">Nombre</th>
                                                        <th class="hidden-xs">Ap Paterno</th>
                                                        <th class="hidden-xs">Ap Materno</th>
                                                        <th class="hidden-xs">Fecha</th>
                                                        <th>usuario Reg</th>
                                                        <th class="hidden-xs">Estado</th>
                                                        
                                                        @*<th>Opciones</th>*@

                                                    </tr>
                                                </thead>
                                                <tbody>

                                                    @foreach (var listar in ListPersNatural)
                                                    {
                                                      
                                                        <tr class="gradeX">
                                                            <td>@listar.PersonaNaturalId</td>
                                                            <td>@listar.DNI</td>
                                                            <td class="hidden-xs">@listar.Nombre</td>
                                                            <td class="hidden-xs">@listar.ApellidoPaterno</td>
                                                            <td class="hidden-xs">@listar.ApellidoMaterno</td>
                                                            <td class="hidden-xs">@listar.FechaRegistro</td>
                                                            <td>@listar.Usuario.Codigo</td>
                                                            <td class="hidden-xs">@listar.Estado</td>
                                                         
                                                            @*<td class="hidden-xs">
                                                                <a class="btn btn-sm btn-primary" href="#">editar</a>

                                                            </td*@
                                                        </tr>
                                                        
                                                    }


                                                </tbody>
                                                <tfoot>
                                                    <tr>
                                                        <th> </th>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                    </tr>
                                                </tfoot>
                                            </table>


                                        }
                                        else
                                        {
                                            <h4> Ningun Dato encontrado </h4>

                                        }

                                    
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



















